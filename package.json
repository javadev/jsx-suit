{
  "name": "jsx-suit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "npm install -g webpack webpack-dev-server && npm install",
    "server": "nodemon --watch server.js --exec babel-node -- server.js",
    "build": "webpack -p --bail",
    "watch": "webpack --progress --watch",
    "watchs": "SOURCE_MAPS=on webpack --progress --watch",
    "hot": "webpack-dev-server --inline --hot --progress",
    "hots": "SOURCE_MAPS=on webpack-dev-server --inline --hot --progress"
  },
  "dependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "express": "^4.10.2",
    "history": "^4.0.0",
    "lodash": "^4.14.0",
    "nodemon": "^1.8.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^4.0.0",
    "webpack": "^3.5.5"
  },
  "devDependencies": {
    "autoimport-loader": "^0.2.4",
    "babel-core": "^6.3.17",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-class-properties": "^6.3.13",
    "babel-plugin-syntax-decorators": "^6.3.13",
    "babel-plugin-syntax-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.28.5",
    "eslint": "^4.4.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^7.2.1",
    "file-loader": "^0.11.2",
    "loader-utils": "^1.1.0",
    "node-sass": "^4.5.3",
    "react-core-decorators": "1.0.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "webpack-dev-server": "^2.7.1"
  }
}
